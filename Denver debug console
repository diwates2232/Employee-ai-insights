const push = async () => {
  console.log('üîÑ [Denver PUSH] Running at:', new Date().toISOString());
  console.log('üîç [Denver] Last seen swipe time:', lastSeen);
  let fresh;
  try {
    fresh = await fetchNewEvents(lastSeen);
    console.log('üì• [Denver] New events fetched:', fresh.length);
    if (fresh.length) {
      console.log('üïí [Denver] Last fresh event time:', fresh[fresh.length - 1].LocaleMessageTime);
    } else {
      console.log('‚ö†Ô∏è [Denver] No new swipes since lastSeen');
    }
  } catch (err) {
    console.error('‚ùå [Denver] Error in fetchNewEvents():', err);
    fresh = [];
  }
  ...
};






const push = async () => {
  const start = Date.now();
  let fresh;
  try {
    fresh = await fetchNewEvents(lastSeen);
    console.log('‚è±Ô∏è [Denver] fetchNewEvents took', Date.now() - start, 'ms');
  } catch (err) {
    console.error('‚ùå [Denver] Error in fetchNewEvents():', err);
    fresh = [];
  }
  ...
};





