After updating all details server.js



const express           = require('express');
const cors              = require('cors');
const path              = require('path');
const listEndpoints     = require('express-list-endpoints');

const employeeRoutes    = require('./routes/employeeRoutes');
const liveOccupancyRt   = require('./routes/liveOccupancyRoutes');

const app = express();
app.use(cors());
app.use(express.json());

// mount each router once, on its own path
app.use('/api/employees',  employeeRoutes);
app.use('/api/occupancy',  liveOccupancyRt);



function listAllEndpoints(app) {
  const results = [];

  function recurse(stack, parentPath = '') {
    stack.forEach(layer => {
      if (layer.route && layer.route.path) {
        // This is a direct route
        const methods = Object.keys(layer.route.methods).map(m => m.toUpperCase());
        results.push({ path: parentPath + layer.route.path, methods });
      }
      else if (layer.name === 'router' && layer.handle.stack) {
        // Mounted router: layer.regexp holds the mount path
        const mountPath = layer.regexp.source
          .replace('^\\','')                // strip leading ^
          .replace('\\/?(?=\\/|$)','')      // strip trailing express regex
          .replace(/\\\//g, '/');           // unescape forward slashes
        recurse(layer.handle.stack, parentPath + mountPath);
      }
    });
  }

  recurse(app._router.stack);
  return results;
}


// â€”â€”â€”â€”â€” DEBUG: list all registered endpoints â€”â€”â€”â€”â€”
console.log('\nðŸ“‹ Registered endpoints:');
listAllEndpoints(app).forEach(e => {
  console.log(`  ${e.methods.join(',')}\t${e.path}`);
});

// serve your React build
const buildPath = path.join(__dirname, '..', 'client', 'build');
app.use(express.static(buildPath));

// simple health check
app.get('/health', (req, res) => res.send('OK'));

const PORT = process.env.PORT || 5000;
app.listen(PORT, () => console.log(`Server running at http://localhost:${PORT}`));



